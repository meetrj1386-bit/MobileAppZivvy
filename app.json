{
  "expo": {
    "name": "Zivvy",
    "slug": "Grow-child-scheduler",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bloom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "25",
      "supportsTablet": true,
      "bundleIdentifier": "com.smartchildbuddy.Zivvy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "fonts": {
      "PlayfairDisplay-Bold": "./assets/fonts/PlayfairDisplay-Bold.ttf",
      "PlayfairDisplay-Regular": "./assets/fonts/PlayfairDisplay-Regular.ttf"
    },
    "developer": {
      "tool": "manual"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.smartchildbuddy.Zivvy",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#667eea"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#667eea"
        }
      ],
      "expo-font",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b3bf505e-5947-4abc-a0b0-0e2b549f10fe"
      }
    },
    "owner": "rahuljain1386"
  }
}
